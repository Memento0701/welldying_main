{
    "name": "welldying",
    "version": "0.1.0",
    "scripts": {
        "client": "yarn --cwd ./client/ watch",
        "server": "yarn --cwd ./server/ watch",
        "watch": "concurrently --kill-others -n \"CLIENT,SERVER\" \"yarn client\" \"yarn server\"",
        "install-all": "yarn && yarn --cwd ./client/ && yarn --cwd ./server/"
    },
    "devDependencies": {
        "concurrently": "^5.3.0"
    },
    "dependencies": {
        "@types/dom-to-image": "^2.6.2",
        "@types/file-saver": "^2.0.2",
        "canvas": "^2.8.0",
        "cross-env": "^7.0.3",
        "dom-to-image": "^2.6.0",
        "file-saver": "^2.0.5",
        "html2canvas": "^1.0.0",
        "i": "^0.3.6",
        "jspdf": "^2.3.1",
        "moment": "^2.29.1",
        "node-sass": "4.14.1",
        "react-external-link": "^1.2.2",
        "react-player": "^2.9.0",
        "react-use": "^17.2.4"
    }
}
